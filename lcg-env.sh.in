COMEDI_DEVICE="/dev/comedi0"
# Choose here the appropriate amplifier for your setup
AMPLIFIER="HEKA-EPC10"
#AMPLIFIER="NPI-ELC"
#AMPLIFIER="AXON-700B"
case $AMPLIFIER in
    "HEKA-EPC10")
        AI_CONVERSION_FACTOR_CC=100
        AO_CONVERSION_FACTOR_CC=0.001
        AI_CONVERSION_FACTOR_VC=1000
        AO_CONVERSION_FACTOR_VC=0.01
	;;
    "AXON-700B")
        AI_CONVERSION_FACTOR_CC=20
        AO_CONVERSION_FACTOR_CC=0.00025
        AI_CONVERSION_FACTOR_VC=400
        AO_CONVERSION_FACTOR_VC=0.05
	;;
	"NPI-ELC")
        AI_CONVERSION_FACTOR_CC=100		# Potential Output Gain set to 100x
        AO_CONVERSION_FACTOR_CC=0.001   # CC (not CC x10)
        AI_CONVERSION_FACTOR_VC=1000	# Current Output Sensitivity 1x 
        AO_CONVERSION_FACTOR_VC=0.01    # VC (not VC x10)
	;;

    *)
        AI_CONVERSION_FACTOR_CC=100
        AO_CONVERSION_FACTOR_CC=0.001
        AI_CONVERSION_FACTOR_VC=1000
        AO_CONVERSION_FACTOR_VC=0.01
	;;
esac
RANGE="[-10,+10]"
AI_SUBDEVICE=0
AI_CHANNEL=0
AI_CHANNEL_VC=0
AI_CHANNEL_CC=0
AO_SUBDEVICE=1
AO_CHANNEL=0
AO_CHANNEL_VC=0
AO_CHANNEL_CC=0
AI_UNITS_CC="mV"
AO_UNITS_CC="pA"
AI_UNITS_VC="pA"
AO_UNITS_VC="mV"
DIGITAL_SUBDEVICE=2
DIGITAL_CHANNEL=0

SAMPLING_RATE=20000
GROUND_REFERENCE="GRSE"
LCG_REALTIME="@with_rt@"
LCG_RESET_OUTPUT="@with_output_reset@"
# KERNEL PROTOCOL DEFAULTS
KERNEL_DUR=10
KERNEL_STD=200
# PREAMBLE DEFAULTS
# small cells
#PREAMBLE_AMPLITUDES="-50,-10"
# big cells
PREAMBLE_AMPLITUDES="-300,-100"
export COMEDI_DEVICE
export AI_CONVERSION_FACTOR_CC
export AO_CONVERSION_FACTOR_CC
export AI_CONVERSION_FACTOR_VC
export AO_CONVERSION_FACTOR_VC
export RANGE
export AI_SUBDEVICE
export AI_CHANNEL
export AI_CHANNEL_CC
export AI_CHANNEL_VC
export AO_SUBDEVICE
export AO_CHANNEL
export AO_CHANNEL_CC
export AO_CHANNEL_VC
export AI_UNITS_CC
export AO_UNITS_CC
export AI_UNITS_VC
export AO_UNITS_VC
export SAMPLING_RATE
export GROUND_REFERENCE
export AMPLIFIER
export LCG_REALTIME
export LCG_RESET_OUTPUT
export DIGITAL_SUBDEVICE
export DIGITAL_CHANNEL
export KERNEL_DUR
export KERNEL_STD
export PREAMBLE_AMPLITUDES
