<dynamicclamp>
  <entities>
    <entity>
      <name>H5Recorder</name>
      <id>0</id>
      <parameters><compress>false</compress></parameters>
    </entity>

    <entity>
      <name>RealNeuron</name>
      <id>1</id>
      <parameters>
	<spikeThreshold>-20</spikeThreshold>
	<V0>-65</V0>
        <delay>2</delay>
	<kernelFile>kernel.dat</kernelFile>
	<deviceFile>/dev/comedi0</deviceFile>
	<inputSubdevice>0</inputSubdevice>
	<outputSubdevice>1</outputSubdevice>
	<inputRange>[-10,+10]</inputRange>
	<readChannel>0</readChannel>
	<writeChannel>0</writeChannel>
	<inputConversionFactor>100</inputConversionFactor>
	<outputConversionFactor>0.001</outputConversionFactor>
	<reference>GRSE</reference>
      </parameters>
      <connections>0,3</connections>
    </entity>

    <entity>
      <name>Waveform</name>
      <id>2</id>
      <parameters><filename>frequency.stim</filename></parameters>
      <connections>0,4</connections>
    </entity>

    <entity>
      <name>FrequencyEstimator</name>
      <id>3</id>
      <parameters>
	<tau>1</tau>
      </parameters>
      <connections>0,4</connections>
    </entity>

    <entity>
      <name>PID</name>
      <id>4</id>
      <parameters>
	<baselineCurrent>I0</baselineCurrent>
	<gp>GP</gp>
	<gi>GI</gi>
	<gd>0</gd>
      </parameters>
      <connections>0,1</connections>
    </entity>

    <entity>
      <name>OUconductance</name>
      <id>5</id>
      <parameters>
	<E>0</E>
	<G0>G0_exc</G0>
	<sigma>sigma_exc</sigma>
	<tau>0.005</tau>
      </parameters>
      <connections>0,1</connections>
    </entity>

    <entity>
      <name>OUconductance</name>
      <id>6</id>
      <parameters>
	<E>-80</E>
	<G0>G0_inh</G0>
	<sigma>sigma_inh</sigma>
	<tau>0.01</tau>
      </parameters>
      <connections>0,1</connections>
    </entity>
 
  </entities>

  <simulation>
    <tend>30</tend>
    <rate>20000</rate>
  </simulation>

</dynamicclamp>
